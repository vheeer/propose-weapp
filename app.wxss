/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page {
  font-family: -apple-system-font, 'Helvetica Neue', Helvetica, 'Microsoft YaHei', sans-serif;
  width: 100%;
  background-color: #e6e6e6;
  display: flex;
  flex-direction: column;
}
image {
  display: block;
  margin: 0px;
}
.hr {
  border-bottom: solid 1rpx #999;
}
.clear:after {
    content:".";
    display:block;
    height:0;
    clear:both;
    visibility:hidden;
}

